            "source": [
                "for current_row in range(0, rows):\n",
                "    for current_col in range(0, cols):\n",
                "\n",
                "        if df.iloc[current_row][current_col] == 0: # found a potential rectangle \n",
                "            bottom = lower_bound(current_row, current_col)\n",
                "            right = right_bound(current_row, current_col)\n",
                "            summation = sum_rectangle(current_row, bottom, current_col, right)\n",
                "            top_product = horizontal_product(current_col, right, current_row-1)\n",
                "            bot_product = horizontal_product(current_col, right, bottom+1)\n",
                "            left_product = vertical_product(current_row, bottom, current_col-1)\n",
                "            # print(right)\n",
                "            right_product = vertical_product(current_row, bottom, right+1)\n",
                "            surround = top_product*bot_product*left_product*right_product\n",
                "            \n",
                "            \n",
                "            if summation != 0 or surround !=1:\n",
                "                continue\n",
                "            \n",
                "            print(current_row, current_col, bottom, right)",
