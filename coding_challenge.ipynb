            "source": [
                "def bottom_bound(row_input, col_input):\n",
                "    relative_row = row_input\n",
                "    \n",
                "    while relative_row < rows-1:\n",
                "        relative_row += 1\n",
                "        below = df.iloc[relative_row][col_input]\n",
                "        # print(below)\n",
                "        if below == 1: # found potential lower boundary\n",
                "            return relative_row -1\n",
                "        \n",
                "    return relative_row -1\n",
                "        \n",
                "def right_bound(row_input, col_input):\n",
                "    relative_col = col_input\n",
                "    \n",
                "    while relative_col < cols-1:\n",
                "        relative_col += 1\n",
                "        right = df.iloc[row_input][relative_col]\n",
                "        # print(below)\n",
                "        if right == 1: # found potential lower boundary\n",
                "            return relative_col - 1\n",
                "    \n",
                "    return relative_col\n",
                "\n",
                "def sum_rectangle(x_1, x_2, y_1, y_2):\n",
                "    summation = 0\n",
                "    for i in range(x_1, x_2+1):\n",
                "        for j in range(y_1, y_2+1):\n",
                "            summation += df.iloc[i][j]\n",
                "            # print(i, j)\n",
                "            # print(summation)\n",
                "    \n",
                "    return summation\n",
                "\n",
                "def horizontal_product(y_1, y_2, x):\n",
                "    if x < 0 or x > rows-1:\n",
                "        return 1\n",
                "    product = 1\n",
                "    for j in range(y_1, y_2+1):\n",
                "        product *= df.iloc[x][j]\n",
                "    return product\n",
                "\n",
                "def vertical_product(x_1, x_2, y):\n",
                "    if y < 0 or y > cols-1:\n",
                "        return 1\n",
                "    product = 1\n",
                "    for i in range(x_1, x_2+1):\n",
                "        product *= df.iloc[i][y]\n",
                "    return product",
            ],
            "metadata": {
